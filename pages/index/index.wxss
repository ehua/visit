/**index.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-sousuo:before { content: "\e618"; }

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.label-1, .label-2 {
  margin-bottom: 15px;
}

.label-1__text, .label-2__text {
  display: inline-block;
  vertical-align: middle;
}

.label-1__icon {
  position: relative;
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
  width: 18px;
  height: 18px;
  background: #fcfff4;
}

.label-1__icon-checked {
  position: absolute;
  top: 3px;
  left: 3px;
  width: 12px;
  height: 12px;
  background: #1aad19;
}

.label-2__icon {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  width: 18px;
  height: 18px;
  background: #fcfff4;
  border-radius: 50px;
}

.label-2__icon-checked {
  position: absolute;
  left: 3px;
  top: 3px;
  width: 12px;
  height: 12px;
  background: #1aad19;
  border-radius: 50%;
}

.label-4_text {
  text-align: center;
  margin-top: 15px;
}

.usermotto {
  margin-top: 200px;
}

.page-section {
  margin-bottom: 20rpx;
}

.flex-wrp {
  display: flex;
}

.bc_green {
  width: 25%;
  height: 5em;
  text-align: center;
  background: green;
}

.bc_red {
  background: red;
  width: 25%;
  height: 5em;
  text-align: center;
}

.bc_blue {
  background: blue;
  width: 25%;
  height: 5em;
  text-align: center;
}

.bc_white {
  background-color: white;
  width: 25%;
  height: 5em;
  text-align: center;
}

.t_tour {
  width: 25%;
  /* height: 10%; */
  background: red;
  text-align: center;
  font-size: 75%;
}

.headView{
  border:  1px solid #0097FF;
  border-radius: 40px;
  height: 40px;
  margin-bottom: 2px;
}

.headFocus{
  padding-top: 7px;
  padding-left: 12px;
}